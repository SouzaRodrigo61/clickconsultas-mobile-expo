{
  "expo": {
    "name": "ClickConsultas",
    "version": "1.0.3",
    "slug": "click-consultas",
    "orientation": "portrait",
    "icon": "./src/images/notificationIcon.png",
    "splash": {
      "image": "./src/images/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#E5E5E5"
    },
    "notification": {
      "icon": "./src/images/notificationIcon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clickconsultasinc.clickconsultas",
      "buildNumber": "27",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.clickconsultasinc.clickconsultas",
      "googleServicesFile": "./google-services.json",
      "versionCode": 26,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAqgD0gn11M2Wz1A2ABnZArPqtclSIQlZI"
        }
      }
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "e81d0fb3-3352-469a-bab5-f114136798c7"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "usesCleartextTraffic": true,
            "networkSecurityConfig": "./network_security_config.xml"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permita que $(PRODUCT_NAME) use sua localização."
        }
      ],
      "expo-font"
    ]
  }
}
